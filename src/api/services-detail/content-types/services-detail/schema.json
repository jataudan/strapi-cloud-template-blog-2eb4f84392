    "singularName": "services-detail",
    "pluralName": "services-details",
    "displayName": "Services Detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "servicesList": {
      "type": "enumeration",
      "enum": [
        "web-development",
        "video-production",
        "seo",
        "email-marketing",
        "digital-strategy",
        "brand-design"
      ],
      "default": "web-development"
